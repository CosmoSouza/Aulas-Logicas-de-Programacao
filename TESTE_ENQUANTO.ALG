Algoritmo "teste_Enquanto"

Var
  x, soma : inteiro


Inicio
     escreva("DIgite o primeiro numero: ")
     leia(x)
     
     soma <- 0
     enquanto x <> 0 faca
     
     soma <- soma + x
     escreva("Digite outro numero ")
     leia(x)
     
     fimenquanto
      escreval("SOMA = ", soma)
Fimalgoritmo